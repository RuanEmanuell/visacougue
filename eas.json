{
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "FIREBASE_API_KEY": "your_firebase_api_key",
        "FIREBASE_AUTH_DOMAIN": "your_firebase_api_key",
        "FIREBASE_PROJECT_ID": "your_firebase_api_key",
        "FIREBASE_STORAGE_BUCKET": "your_firebase_api_key",
        "FIREBASE_MESSAGING_SENDER_ID": "your_firebase_api_key",
        "FIREBASE_APP_ID": "your_firebase_api_key"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "FIREBASE_API_KEY": "your_firebase_api_key",
        "FIREBASE_AUTH_DOMAIN": "your_firebase_api_key",
        "FIREBASE_PROJECT_ID": "your_firebase_api_key",
        "FIREBASE_STORAGE_BUCKET": "your_firebase_api_key",
        "FIREBASE_MESSAGING_SENDER_ID": "your_firebase_api_key",
        "FIREBASE_APP_ID": "your_firebase_api_key"
      },
      "android" : {
        "buildType" : "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
